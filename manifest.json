{
  "manifest_version": 3,
  "name": "ImPares",
  "version": "1.0",
  "description": "Descarga masiva de im√°genes de Pares",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads"
  ],
  "action": {
    "default_popup": "popup.html",
    "matches": [
      "http://pares.mcu.es/ParesBusquedas20/catalogo/show/*"
    ],
    "default_icon": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    }
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "background": {
    "service_worker": "js/background.js"
  }
}
